package entities;

public class funcionario {
	
	public String Name;
	public double GrossSalary;
	public double Tax;
	
	public double NetSalary() {
		double liquido = GrossSalary - Tax;
		return liquido;
	}
	public void increaseSalary() {
		double porcentagem = (porcentagem * liquido) / 100;
	}
	
	
	
}
