package entities;

public class aluno {
	public String Name;
	public double nota1;
	public double nota2;
	public double nota3;
	
	
	
	public double notaFinal() {
		double notaFinal = nota1 + nota2 + nota3;
		return notaFinal;
	}
		
	public double result() {
			double resultado = notaFinal() * 0.6;
			return resultado;
		}
		
	}

	
	

