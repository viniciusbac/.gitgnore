package entities;

public class Client {

	public int number;
	public String name;
	public double valorInicial;

	public double deposit() {
		return valorInicial + deposit();
	}

	public double withdraw() {
		return deposit() - withdraw() ;
	}

	public String toString() {
		return "Account " 
			+ number 
			+ ", "
			+ "Holder: " 
			+ name + ", " 
			+ "Balance: $ ";
	}
}
